from selenium.webdriver.common.action_chains import ActionChains

def get_action_chains():
    actions = []
    for item in dir(ActionChains):
        if item[0:2] != '__':
            actions.append(item)
    return actions

click
click_and_hold
context_click
double_click
drag_and_drop
drag_and_drop_by_offset
key_down
key_up
move_by_offset
move_to_element
move_to_element_with_offset
